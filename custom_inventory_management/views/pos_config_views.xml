<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="view_pos_order_form_custom" model="ir.ui.view">
        <field name="name">pos.order.form.custom</field>
        <field name="model">pos.order</field>
        <field name="inherit_id" ref="point_of_sale.view_pos_order_form"/>
        <field name="arch" type="xml">
            <!-- Add fields at the end of the form -->
            <xpath expr="//form" position="inside">
                <group string="Custom Information">
                    <field name="custom_receipt_note"/>
                    <field name="delivery_preference"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>